PROCEDURE "procedures.utils::PR_CONF_GET_PREDICTION_DATE" ( 
OUT O_START_DATE	TIMESTAMP	)
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   READS SQL DATA AS
BEGIN
   -- Get Current Active DATE
	LT_CONFIG = SELECT * FROM "models.adm::CV_PREDICTION_DATE" WHERE "IS_ENABLED" = 1;
	
	IF IS_EMPTY(:LT_CONFIG) 
	THEN
		RETURN;
	ELSE
		SELECT "TO_DATE" INTO O_START_DATE FROM :LT_CONFIG;
	END IF;	
END