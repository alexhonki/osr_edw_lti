PROCEDURE "procedures.utils::PR_CONF_GET_REACTION_DAYS" ( out IP_REACTION_DAYS varchar(6) ) 
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   READS SQL DATA AS
BEGIN
   SELECT TOP 1 TO_VARCHAR(COALESCE("FROM_VALUE","TO_VALUE","FROM_DEFAULT")) 
	 	INTO IP_REACTION_DAYS 
	 	FROM "models.adm::CV_CONFIG"
	 	where 	"BUCKET_ID" = 'REACTION_DAYS'
	 	AND 	"REACTION_TYPE" IN (SELECT "REACTION_TYPE" FROM "functions::TF_CONF_GET_REACTION_TYPE" ( ));
END