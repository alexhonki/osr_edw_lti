{
	"_version": "1.1.0",
	"sap.fiori": {
		"_version": "1.1.0",
		"registrationIds": [],
		"archeType": "transactional"
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.platform.runtime": {
		"componentProperties": {
			"url": "/app1/sap/demo/sample"
		}
	},
	"sap.app": {
		"title": "Personalization Demo",
		"id": "app1",
		"dataSources": {
			"Products": {
				"type": "JSON",
				"uri": "./Products.json"
			}
		},
		"crossNavigation": {
			"inbounds": {
				"app1Show": {
					"semanticObject": "app1",
					"action": "show",
					"deviceTypes": {
						"desktop": true,
						"tablet": true,
						"phone": true
					}
				}
			},
			"outbounds": {

			}
		}
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"componentName": "sap.demo.sample",
		"rootView": {
			"viewName": "sap.demo.sample.view.PersonalizationApp",
			"type": "XML",
			"id": "PersonalizationApp"
		},
		"config": {
			"fullWidth": true
		},
		"dependencies": {
			"minUI5Version": "1.32.7",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"": {
				"dataSource": "Products",
				"type": "sap.ui.model.json.JSONModel"
			}
		}
	}
}